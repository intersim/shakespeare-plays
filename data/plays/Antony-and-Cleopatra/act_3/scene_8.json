[{"line_id":20616,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"3.8.1","speaker":"OCTAVIUS CAESAR","text_entry":"Taurus!"},{"line_id":20617,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"3.8.2","speaker":"TAURUS","text_entry":"My lord?"},{"line_id":20618,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"3.8.3","speaker":"OCTAVIUS CAESAR","text_entry":"Strike not by land; keep whole: provoke not battle,"},{"line_id":20619,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"3.8.4","speaker":"OCTAVIUS CAESAR","text_entry":"Till we have done at sea. Do not exceed"},{"line_id":20620,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"3.8.5","speaker":"OCTAVIUS CAESAR","text_entry":"The prescript of this scroll: our fortune lies"},{"line_id":20621,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"3.8.6","speaker":"OCTAVIUS CAESAR","text_entry":"Upon this jump."},{"line_id":20622,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"3.8.0","speaker":"OCTAVIUS CAESAR","text_entry":"Exeunt"}]