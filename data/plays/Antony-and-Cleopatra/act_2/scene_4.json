[{"line_id":19601,"play_name":"Antony and Cleopatra","speech_number":13,"line_number":"2.4.0","speaker":"MARK ANTONY","text_entry":"SCENE IV. The same. A street."},{"line_id":19602,"play_name":"Antony and Cleopatra","speech_number":13,"line_number":"2.4.0","speaker":"MARK ANTONY","text_entry":"Enter LEPIDUS, MECAENAS, and AGRIPPA"},{"line_id":19603,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"2.4.1","speaker":"LEPIDUS","text_entry":"Trouble yourselves no further: pray you, hasten"},{"line_id":19604,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"2.4.2","speaker":"LEPIDUS","text_entry":"Your generals after."},{"line_id":19605,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"2.4.3","speaker":"AGRIPPA","text_entry":"Sir, Mark Antony"},{"line_id":19606,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"2.4.4","speaker":"AGRIPPA","text_entry":"Will e'en but kiss Octavia, and we'll follow."},{"line_id":19607,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"2.4.5","speaker":"LEPIDUS","text_entry":"Till I shall see you in your soldier's dress,"},{"line_id":19608,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"2.4.6","speaker":"LEPIDUS","text_entry":"Which will become you both, farewell."},{"line_id":19609,"play_name":"Antony and Cleopatra","speech_number":4,"line_number":"2.4.7","speaker":"MECAENAS","text_entry":"We shall,"},{"line_id":19610,"play_name":"Antony and Cleopatra","speech_number":4,"line_number":"2.4.8","speaker":"MECAENAS","text_entry":"As I conceive the journey, be at the Mount"},{"line_id":19611,"play_name":"Antony and Cleopatra","speech_number":4,"line_number":"2.4.9","speaker":"MECAENAS","text_entry":"Before you, Lepidus."},{"line_id":19612,"play_name":"Antony and Cleopatra","speech_number":5,"line_number":"2.4.10","speaker":"LEPIDUS","text_entry":"Your way is shorter;"},{"line_id":19613,"play_name":"Antony and Cleopatra","speech_number":5,"line_number":"2.4.11","speaker":"LEPIDUS","text_entry":"My purposes do draw me much about:"},{"line_id":19614,"play_name":"Antony and Cleopatra","speech_number":5,"line_number":"2.4.12","speaker":"LEPIDUS","text_entry":"You'll win two days upon me."},{"line_id":19615,"play_name":"Antony and Cleopatra","speech_number":7,"line_number":"2.4.13","speaker":"AGRIPPA","text_entry":"Sir, good success!"},{"line_id":19616,"play_name":"Antony and Cleopatra","speech_number":8,"line_number":"2.4.14","speaker":"LEPIDUS","text_entry":"Farewell."},{"line_id":19617,"play_name":"Antony and Cleopatra","speech_number":8,"line_number":"2.4.0","speaker":"LEPIDUS","text_entry":"Exeunt"}]