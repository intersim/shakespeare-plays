[{"line_id":21406,"play_name":"Antony and Cleopatra","speech_number":5,"line_number":"4.9.0","speaker":"MARK ANTONY","text_entry":"SCENE IX. OCTAVIUS CAESAR's camp."},{"line_id":21407,"play_name":"Antony and Cleopatra","speech_number":5,"line_number":"4.9.0","speaker":"MARK ANTONY","text_entry":"Sentinels at their post"},{"line_id":21408,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"4.9.1","speaker":"First Soldier","text_entry":"If we be not relieved within this hour,"},{"line_id":21409,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"4.9.2","speaker":"First Soldier","text_entry":"We must return to the court of guard: the night"},{"line_id":21410,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"4.9.3","speaker":"First Soldier","text_entry":"Is shiny; and they say we shall embattle"},{"line_id":21411,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"4.9.4","speaker":"First Soldier","text_entry":"By the second hour i' the morn."},{"line_id":21412,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"4.9.5","speaker":"Second Soldier","text_entry":"This last day was"},{"line_id":21413,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"4.9.6","speaker":"Second Soldier","text_entry":"A shrewd one to's."},{"line_id":21414,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"4.9.0","speaker":"Second Soldier","text_entry":"Enter DOMITIUS ENOBARBUS"},{"line_id":21415,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"4.9.7","speaker":"DOMITIUS ENOBARBUS","text_entry":"O, bear me witness, night,--"},{"line_id":21416,"play_name":"Antony and Cleopatra","speech_number":4,"line_number":"4.9.8","speaker":"Third Soldier","text_entry":"What man is this?"},{"line_id":21417,"play_name":"Antony and Cleopatra","speech_number":5,"line_number":"4.9.9","speaker":"Second Soldier","text_entry":"Stand close, and list him."},{"line_id":21418,"play_name":"Antony and Cleopatra","speech_number":6,"line_number":"4.9.10","speaker":"DOMITIUS ENOBARBUS","text_entry":"Be witness to me, O thou blessed moon,"},{"line_id":21419,"play_name":"Antony and Cleopatra","speech_number":6,"line_number":"4.9.11","speaker":"DOMITIUS ENOBARBUS","text_entry":"When men revolted shall upon record"},{"line_id":21420,"play_name":"Antony and Cleopatra","speech_number":6,"line_number":"4.9.12","speaker":"DOMITIUS ENOBARBUS","text_entry":"Bear hateful memory, poor Enobarbus did"},{"line_id":21421,"play_name":"Antony and Cleopatra","speech_number":6,"line_number":"4.9.13","speaker":"DOMITIUS ENOBARBUS","text_entry":"Before thy face repent!"},{"line_id":21422,"play_name":"Antony and Cleopatra","speech_number":7,"line_number":"4.9.14","speaker":"First Soldier","text_entry":"Enobarbus!"},{"line_id":21423,"play_name":"Antony and Cleopatra","speech_number":8,"line_number":"4.9.15","speaker":"Third Soldier","text_entry":"Peace!"},{"line_id":21424,"play_name":"Antony and Cleopatra","speech_number":8,"line_number":"4.9.16","speaker":"Third Soldier","text_entry":"Hark further."},{"line_id":21425,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.17","speaker":"DOMITIUS ENOBARBUS","text_entry":"O sovereign mistress of true melancholy,"},{"line_id":21426,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.18","speaker":"DOMITIUS ENOBARBUS","text_entry":"The poisonous damp of night disponge upon me,"},{"line_id":21427,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.19","speaker":"DOMITIUS ENOBARBUS","text_entry":"That life, a very rebel to my will,"},{"line_id":21428,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.20","speaker":"DOMITIUS ENOBARBUS","text_entry":"May hang no longer on me: throw my heart"},{"line_id":21429,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.21","speaker":"DOMITIUS ENOBARBUS","text_entry":"Against the flint and hardness of my fault:"},{"line_id":21430,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.22","speaker":"DOMITIUS ENOBARBUS","text_entry":"Which, being dried with grief, will break to powder,"},{"line_id":21431,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.23","speaker":"DOMITIUS ENOBARBUS","text_entry":"And finish all foul thoughts. O Antony,"},{"line_id":21432,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.24","speaker":"DOMITIUS ENOBARBUS","text_entry":"Nobler than my revolt is infamous,"},{"line_id":21433,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.25","speaker":"DOMITIUS ENOBARBUS","text_entry":"Forgive me in thine own particular;"},{"line_id":21434,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.26","speaker":"DOMITIUS ENOBARBUS","text_entry":"But let the world rank me in register"},{"line_id":21435,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.27","speaker":"DOMITIUS ENOBARBUS","text_entry":"A master-leaver and a fugitive:"},{"line_id":21436,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.28","speaker":"DOMITIUS ENOBARBUS","text_entry":"O Antony! O Antony!"},{"line_id":21437,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.9.0","speaker":"DOMITIUS ENOBARBUS","text_entry":"Dies"},{"line_id":21438,"play_name":"Antony and Cleopatra","speech_number":10,"line_number":"4.9.29","speaker":"Second Soldier","text_entry":"Let's speak To him."},{"line_id":21439,"play_name":"Antony and Cleopatra","speech_number":11,"line_number":"4.9.30","speaker":"First Soldier","text_entry":"Let's hear him, for the things he speaks"},{"line_id":21440,"play_name":"Antony and Cleopatra","speech_number":11,"line_number":"4.9.31","speaker":"First Soldier","text_entry":"May concern Caesar."},{"line_id":21441,"play_name":"Antony and Cleopatra","speech_number":12,"line_number":"4.9.32","speaker":"Third Soldier","text_entry":"Let's do so. But he sleeps."},{"line_id":21442,"play_name":"Antony and Cleopatra","speech_number":13,"line_number":"4.9.33","speaker":"First Soldier","text_entry":"Swoons rather; for so bad a prayer as his"},{"line_id":21443,"play_name":"Antony and Cleopatra","speech_number":13,"line_number":"4.9.34","speaker":"First Soldier","text_entry":"Was never yet for sleep."},{"line_id":21444,"play_name":"Antony and Cleopatra","speech_number":14,"line_number":"4.9.35","speaker":"Second Soldier","text_entry":"Go we to him."},{"line_id":21445,"play_name":"Antony and Cleopatra","speech_number":15,"line_number":"4.9.36","speaker":"Third Soldier","text_entry":"Awake, sir, awake; speak to us."},{"line_id":21446,"play_name":"Antony and Cleopatra","speech_number":16,"line_number":"4.9.37","speaker":"Second Soldier","text_entry":"Hear you, sir?"},{"line_id":21447,"play_name":"Antony and Cleopatra","speech_number":17,"line_number":"4.9.38","speaker":"First Soldier","text_entry":"The hand of death hath raught him."},{"line_id":21448,"play_name":"Antony and Cleopatra","speech_number":17,"line_number":"4.9.0","speaker":"First Soldier","text_entry":"Drums afar off"},{"line_id":21449,"play_name":"Antony and Cleopatra","speech_number":17,"line_number":"4.9.39","speaker":"First Soldier","text_entry":"Hark! the drums"},{"line_id":21450,"play_name":"Antony and Cleopatra","speech_number":17,"line_number":"4.9.40","speaker":"First Soldier","text_entry":"Demurely wake the sleepers. Let us bear him"},{"line_id":21451,"play_name":"Antony and Cleopatra","speech_number":17,"line_number":"4.9.41","speaker":"First Soldier","text_entry":"To the court of guard; he is of note: our hour"},{"line_id":21452,"play_name":"Antony and Cleopatra","speech_number":17,"line_number":"4.9.42","speaker":"First Soldier","text_entry":"Is fully out."},{"line_id":21453,"play_name":"Antony and Cleopatra","speech_number":18,"line_number":"4.9.43","speaker":"Third Soldier","text_entry":"Come on, then;"},{"line_id":21454,"play_name":"Antony and Cleopatra","speech_number":18,"line_number":"4.9.44","speaker":"Third Soldier","text_entry":"He may recover yet."},{"line_id":21455,"play_name":"Antony and Cleopatra","speech_number":18,"line_number":"4.9.0","speaker":"Third Soldier","text_entry":"Exeunt with the body"}]