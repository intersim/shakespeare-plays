[{"line_id":21328,"play_name":"Antony and Cleopatra","speech_number":10,"line_number":"4.7.0","speaker":"DOMITIUS ENOBARBUS","text_entry":"SCENE VII. Field of battle between the camps."},{"line_id":21329,"play_name":"Antony and Cleopatra","speech_number":10,"line_number":"4.7.0","speaker":"DOMITIUS ENOBARBUS","text_entry":"Alarum. Drums and trumpets. Enter AGRIPPA and others"},{"line_id":21330,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"4.7.1","speaker":"AGRIPPA","text_entry":"Retire, we have engaged ourselves too far:"},{"line_id":21331,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"4.7.2","speaker":"AGRIPPA","text_entry":"Caesar himself has work, and our oppression"},{"line_id":21332,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"4.7.3","speaker":"AGRIPPA","text_entry":"Exceeds what we expected."},{"line_id":21333,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"4.7.0","speaker":"AGRIPPA","text_entry":"Exeunt"},{"line_id":21334,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"4.7.0","speaker":"AGRIPPA","text_entry":"Alarums. Enter MARK ANTONY and SCARUS wounded"},{"line_id":21335,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"4.7.4","speaker":"SCARUS","text_entry":"O my brave emperor, this is fought indeed!"},{"line_id":21336,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"4.7.5","speaker":"SCARUS","text_entry":"Had we done so at first, we had droven them home"},{"line_id":21337,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"4.7.6","speaker":"SCARUS","text_entry":"With clouts about their heads."},{"line_id":21338,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"4.7.7","speaker":"MARK ANTONY","text_entry":"Thou bleed'st apace."},{"line_id":21339,"play_name":"Antony and Cleopatra","speech_number":4,"line_number":"4.7.8","speaker":"SCARUS","text_entry":"I had a wound here that was like a T,"},{"line_id":21340,"play_name":"Antony and Cleopatra","speech_number":4,"line_number":"4.7.9","speaker":"SCARUS","text_entry":"But now 'tis made an H."},{"line_id":21341,"play_name":"Antony and Cleopatra","speech_number":5,"line_number":"4.7.10","speaker":"MARK ANTONY","text_entry":"They do retire."},{"line_id":21342,"play_name":"Antony and Cleopatra","speech_number":6,"line_number":"4.7.11","speaker":"SCARUS","text_entry":"We'll beat 'em into bench-holes: I have yet"},{"line_id":21343,"play_name":"Antony and Cleopatra","speech_number":6,"line_number":"4.7.12","speaker":"SCARUS","text_entry":"Room for six scotches more."},{"line_id":21344,"play_name":"Antony and Cleopatra","speech_number":6,"line_number":"4.7.0","speaker":"SCARUS","text_entry":"Enter EROS"},{"line_id":21345,"play_name":"Antony and Cleopatra","speech_number":7,"line_number":"4.7.13","speaker":"EROS","text_entry":"They are beaten, sir, and our advantage serves"},{"line_id":21346,"play_name":"Antony and Cleopatra","speech_number":7,"line_number":"4.7.14","speaker":"EROS","text_entry":"For a fair victory."},{"line_id":21347,"play_name":"Antony and Cleopatra","speech_number":8,"line_number":"4.7.15","speaker":"SCARUS","text_entry":"Let us score their backs,"},{"line_id":21348,"play_name":"Antony and Cleopatra","speech_number":8,"line_number":"4.7.16","speaker":"SCARUS","text_entry":"And snatch 'em up, as we take hares, behind:"},{"line_id":21349,"play_name":"Antony and Cleopatra","speech_number":8,"line_number":"4.7.17","speaker":"SCARUS","text_entry":"'Tis sport to maul a runner."},{"line_id":21350,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.7.18","speaker":"MARK ANTONY","text_entry":"I will reward thee"},{"line_id":21351,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.7.19","speaker":"MARK ANTONY","text_entry":"Once for thy spritely comfort, and ten-fold"},{"line_id":21352,"play_name":"Antony and Cleopatra","speech_number":9,"line_number":"4.7.20","speaker":"MARK ANTONY","text_entry":"For thy good valour. Come thee on."},{"line_id":21353,"play_name":"Antony and Cleopatra","speech_number":10,"line_number":"4.7.21","speaker":"SCARUS","text_entry":"I'll halt after."},{"line_id":21354,"play_name":"Antony and Cleopatra","speech_number":10,"line_number":"4.7.0","speaker":"SCARUS","text_entry":"Exeunt"}]