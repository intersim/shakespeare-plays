[{"line_id":21540,"play_name":"Antony and Cleopatra","speech_number":5,"line_number":"4.13.0","speaker":"MARK ANTONY","text_entry":"SCENE XIII. Alexandria. Cleopatra's palace."},{"line_id":21541,"play_name":"Antony and Cleopatra","speech_number":5,"line_number":"4.13.0","speaker":"MARK ANTONY","text_entry":"Enter CLEOPATRA, CHARMIAN, IRAS, and MARDIAN"},{"line_id":21542,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"4.13.1","speaker":"CLEOPATRA","text_entry":"Help me, my women! O, he is more mad"},{"line_id":21543,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"4.13.2","speaker":"CLEOPATRA","text_entry":"Than Telamon for his shield; the boar of Thessaly"},{"line_id":21544,"play_name":"Antony and Cleopatra","speech_number":1,"line_number":"4.13.3","speaker":"CLEOPATRA","text_entry":"Was never so emboss'd."},{"line_id":21545,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"4.13.4","speaker":"CHARMIAN","text_entry":"To the monument!"},{"line_id":21546,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"4.13.5","speaker":"CHARMIAN","text_entry":"There lock yourself, and send him word you are dead."},{"line_id":21547,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"4.13.6","speaker":"CHARMIAN","text_entry":"The soul and body rive not more in parting"},{"line_id":21548,"play_name":"Antony and Cleopatra","speech_number":2,"line_number":"4.13.7","speaker":"CHARMIAN","text_entry":"Than greatness going off."},{"line_id":21549,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"4.13.8","speaker":"CLEOPATRA","text_entry":"To the monument!"},{"line_id":21550,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"4.13.9","speaker":"CLEOPATRA","text_entry":"Mardian, go tell him I have slain myself;"},{"line_id":21551,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"4.13.10","speaker":"CLEOPATRA","text_entry":"Say, that the last I spoke was 'Antony,'"},{"line_id":21552,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"4.13.11","speaker":"CLEOPATRA","text_entry":"And word it, prithee, piteously: hence, Mardian,"},{"line_id":21553,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"4.13.12","speaker":"CLEOPATRA","text_entry":"And bring me how he takes my death."},{"line_id":21554,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"4.13.13","speaker":"CLEOPATRA","text_entry":"To the monument!"},{"line_id":21555,"play_name":"Antony and Cleopatra","speech_number":3,"line_number":"4.13.0","speaker":"CLEOPATRA","text_entry":"Exeunt"}]