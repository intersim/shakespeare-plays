[{"line_id":14208,"play_name":"Alls well that ends well","speech_number":42,"line_number":"3.3.0","speaker":"HELENA","text_entry":"SCENE III. Florence. Before the DUKE's palace."},{"line_id":14209,"play_name":"Alls well that ends well","speech_number":42,"line_number":"3.3.0","speaker":"HELENA","text_entry":"Flourish. Enter the DUKE of Florence, BERTRAM, PAROLLES, Soldiers, Drum, and Trumpets"},{"line_id":14210,"play_name":"Alls well that ends well","speech_number":1,"line_number":"3.3.1","speaker":"DUKE","text_entry":"The general of our horse thou art; and we,"},{"line_id":14211,"play_name":"Alls well that ends well","speech_number":1,"line_number":"3.3.2","speaker":"DUKE","text_entry":"Great in our hope, lay our best love and credence"},{"line_id":14212,"play_name":"Alls well that ends well","speech_number":1,"line_number":"3.3.3","speaker":"DUKE","text_entry":"Upon thy promising fortune."},{"line_id":14213,"play_name":"Alls well that ends well","speech_number":2,"line_number":"3.3.4","speaker":"BERTRAM","text_entry":"Sir, it is"},{"line_id":14214,"play_name":"Alls well that ends well","speech_number":2,"line_number":"3.3.5","speaker":"BERTRAM","text_entry":"A charge too heavy for my strength, but yet"},{"line_id":14215,"play_name":"Alls well that ends well","speech_number":2,"line_number":"3.3.6","speaker":"BERTRAM","text_entry":"We'll strive to bear it for your worthy sake"},{"line_id":14216,"play_name":"Alls well that ends well","speech_number":2,"line_number":"3.3.7","speaker":"BERTRAM","text_entry":"To the extreme edge of hazard."},{"line_id":14217,"play_name":"Alls well that ends well","speech_number":3,"line_number":"3.3.8","speaker":"DUKE","text_entry":"Then go thou forth;"},{"line_id":14218,"play_name":"Alls well that ends well","speech_number":3,"line_number":"3.3.9","speaker":"DUKE","text_entry":"And fortune play upon thy prosperous helm,"},{"line_id":14219,"play_name":"Alls well that ends well","speech_number":3,"line_number":"3.3.10","speaker":"DUKE","text_entry":"As thy auspicious mistress!"},{"line_id":14220,"play_name":"Alls well that ends well","speech_number":4,"line_number":"3.3.11","speaker":"BERTRAM","text_entry":"This very day,"},{"line_id":14221,"play_name":"Alls well that ends well","speech_number":4,"line_number":"3.3.12","speaker":"BERTRAM","text_entry":"Great Mars, I put myself into thy file:"},{"line_id":14222,"play_name":"Alls well that ends well","speech_number":4,"line_number":"3.3.13","speaker":"BERTRAM","text_entry":"Make me but like my thoughts, and I shall prove"},{"line_id":14223,"play_name":"Alls well that ends well","speech_number":4,"line_number":"3.3.14","speaker":"BERTRAM","text_entry":"A lover of thy drum, hater of love."},{"line_id":14224,"play_name":"Alls well that ends well","speech_number":4,"line_number":"3.3.0","speaker":"BERTRAM","text_entry":"Exeunt"}]