[{"line_id":66740,"play_name":"Merry Wives of Windsor","speech_number":10,"line_number":"5.4.0","speaker":"MISTRESS FORD","text_entry":"SCENE IV. Windsor Park."},{"line_id":66741,"play_name":"Merry Wives of Windsor","speech_number":10,"line_number":"5.4.0","speaker":"MISTRESS FORD","text_entry":"Enter SIR HUGH EVANS, disguised, with others as Fairies"},{"line_id":66742,"play_name":"Merry Wives of Windsor","speech_number":1,"line_number":"5.4.1","speaker":"SIR HUGH EVANS","text_entry":"Trib, trib, fairies; come; and remember your parts:"},{"line_id":66743,"play_name":"Merry Wives of Windsor","speech_number":1,"line_number":"5.4.2","speaker":"SIR HUGH EVANS","text_entry":"be pold, I pray you; follow me into the pit; and"},{"line_id":66744,"play_name":"Merry Wives of Windsor","speech_number":1,"line_number":"5.4.3","speaker":"SIR HUGH EVANS","text_entry":"when I give the watch-'ords, do as I pid you:"},{"line_id":66745,"play_name":"Merry Wives of Windsor","speech_number":1,"line_number":"5.4.4","speaker":"SIR HUGH EVANS","text_entry":"come, come; trib, trib."},{"line_id":66746,"play_name":"Merry Wives of Windsor","speech_number":1,"line_number":"5.4.0","speaker":"SIR HUGH EVANS","text_entry":"Exeunt"}]