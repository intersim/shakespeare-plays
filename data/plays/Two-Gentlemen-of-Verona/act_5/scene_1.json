[{"line_id":107618,"play_name":"Two Gentlemen of Verona","speech_number":47,"line_number":"5.1.0","speaker":"JULIA","text_entry":"ACT V"},{"line_id":107619,"play_name":"Two Gentlemen of Verona","speech_number":47,"line_number":"5.1.0","speaker":"JULIA","text_entry":"SCENE I. Milan. An abbey."},{"line_id":107620,"play_name":"Two Gentlemen of Verona","speech_number":47,"line_number":"5.1.0","speaker":"JULIA","text_entry":"Enter EGLAMOUR"},{"line_id":107621,"play_name":"Two Gentlemen of Verona","speech_number":1,"line_number":"5.1.1","speaker":"EGLAMOUR","text_entry":"The sun begins to gild the western sky;"},{"line_id":107622,"play_name":"Two Gentlemen of Verona","speech_number":1,"line_number":"5.1.2","speaker":"EGLAMOUR","text_entry":"And now it is about the very hour"},{"line_id":107623,"play_name":"Two Gentlemen of Verona","speech_number":1,"line_number":"5.1.3","speaker":"EGLAMOUR","text_entry":"That Silvia, at Friar Patrick's cell, should meet me."},{"line_id":107624,"play_name":"Two Gentlemen of Verona","speech_number":1,"line_number":"5.1.4","speaker":"EGLAMOUR","text_entry":"She will not fail, for lovers break not hours,"},{"line_id":107625,"play_name":"Two Gentlemen of Verona","speech_number":1,"line_number":"5.1.5","speaker":"EGLAMOUR","text_entry":"Unless it be to come before their time;"},{"line_id":107626,"play_name":"Two Gentlemen of Verona","speech_number":1,"line_number":"5.1.6","speaker":"EGLAMOUR","text_entry":"So much they spur their expedition."},{"line_id":107627,"play_name":"Two Gentlemen of Verona","speech_number":1,"line_number":"5.1.7","speaker":"EGLAMOUR","text_entry":"See where she comes."},{"line_id":107628,"play_name":"Two Gentlemen of Verona","speech_number":1,"line_number":"5.1.0","speaker":"EGLAMOUR","text_entry":"Enter SILVIA"},{"line_id":107629,"play_name":"Two Gentlemen of Verona","speech_number":1,"line_number":"5.1.8","speaker":"EGLAMOUR","text_entry":"Lady, a happy evening!"},{"line_id":107630,"play_name":"Two Gentlemen of Verona","speech_number":2,"line_number":"5.1.9","speaker":"SILVIA","text_entry":"Amen, amen! Go on, good Eglamour,"},{"line_id":107631,"play_name":"Two Gentlemen of Verona","speech_number":2,"line_number":"5.1.10","speaker":"SILVIA","text_entry":"Out at the postern by the abbey-wall:"},{"line_id":107632,"play_name":"Two Gentlemen of Verona","speech_number":2,"line_number":"5.1.11","speaker":"SILVIA","text_entry":"I fear I am attended by some spies."},{"line_id":107633,"play_name":"Two Gentlemen of Verona","speech_number":3,"line_number":"5.1.12","speaker":"EGLAMOUR","text_entry":"Fear not: the forest is not three leagues off;"},{"line_id":107634,"play_name":"Two Gentlemen of Verona","speech_number":3,"line_number":"5.1.13","speaker":"EGLAMOUR","text_entry":"If we recover that, we are sure enough."},{"line_id":107635,"play_name":"Two Gentlemen of Verona","speech_number":3,"line_number":"5.1.0","speaker":"EGLAMOUR","text_entry":"Exeunt"}]