[{"line_id":57024,"play_name":"Macbeth","speech_number":14,"line_number":"3.3.0","speaker":"MACBETH","text_entry":"SCENE III. A park near the palace."},{"line_id":57025,"play_name":"Macbeth","speech_number":14,"line_number":"3.3.0","speaker":"MACBETH","text_entry":"Enter three Murderers"},{"line_id":57026,"play_name":"Macbeth","speech_number":1,"line_number":"3.3.1","speaker":"First Murderer","text_entry":"But who did bid thee join with us?"},{"line_id":57027,"play_name":"Macbeth","speech_number":2,"line_number":"3.3.2","speaker":"Third Murderer","text_entry":"Macbeth."},{"line_id":57028,"play_name":"Macbeth","speech_number":3,"line_number":"3.3.3","speaker":"Second Murderer","text_entry":"He needs not our mistrust, since he delivers"},{"line_id":57029,"play_name":"Macbeth","speech_number":3,"line_number":"3.3.4","speaker":"Second Murderer","text_entry":"Our offices and what we have to do"},{"line_id":57030,"play_name":"Macbeth","speech_number":3,"line_number":"3.3.5","speaker":"Second Murderer","text_entry":"To the direction just."},{"line_id":57031,"play_name":"Macbeth","speech_number":4,"line_number":"3.3.6","speaker":"First Murderer","text_entry":"Then stand with us."},{"line_id":57032,"play_name":"Macbeth","speech_number":4,"line_number":"3.3.7","speaker":"First Murderer","text_entry":"The west yet glimmers with some streaks of day:"},{"line_id":57033,"play_name":"Macbeth","speech_number":4,"line_number":"3.3.8","speaker":"First Murderer","text_entry":"Now spurs the lated traveller apace"},{"line_id":57034,"play_name":"Macbeth","speech_number":4,"line_number":"3.3.9","speaker":"First Murderer","text_entry":"To gain the timely inn; and near approaches"},{"line_id":57035,"play_name":"Macbeth","speech_number":4,"line_number":"3.3.10","speaker":"First Murderer","text_entry":"The subject of our watch."},{"line_id":57036,"play_name":"Macbeth","speech_number":5,"line_number":"3.3.11","speaker":"Third Murderer","text_entry":"Hark! I hear horses."},{"line_id":57037,"play_name":"Macbeth","speech_number":6,"line_number":"3.3.12","speaker":"BANQUO","text_entry":"[Within]  Give us a light there, ho!"},{"line_id":57038,"play_name":"Macbeth","speech_number":7,"line_number":"3.3.13","speaker":"Second Murderer","text_entry":"Then 'tis he: the rest"},{"line_id":57039,"play_name":"Macbeth","speech_number":7,"line_number":"3.3.14","speaker":"Second Murderer","text_entry":"That are within the note of expectation"},{"line_id":57040,"play_name":"Macbeth","speech_number":7,"line_number":"3.3.15","speaker":"Second Murderer","text_entry":"Already are i' the court."},{"line_id":57041,"play_name":"Macbeth","speech_number":8,"line_number":"3.3.16","speaker":"First Murderer","text_entry":"His horses go about."},{"line_id":57042,"play_name":"Macbeth","speech_number":9,"line_number":"3.3.17","speaker":"Third Murderer","text_entry":"Almost a mile: but he does usually,"},{"line_id":57043,"play_name":"Macbeth","speech_number":9,"line_number":"3.3.18","speaker":"Third Murderer","text_entry":"So all men do, from hence to the palace gate"},{"line_id":57044,"play_name":"Macbeth","speech_number":9,"line_number":"3.3.19","speaker":"Third Murderer","text_entry":"Make it their walk."},{"line_id":57045,"play_name":"Macbeth","speech_number":10,"line_number":"3.3.20","speaker":"Second Murderer","text_entry":"A light, a light!"},{"line_id":57046,"play_name":"Macbeth","speech_number":10,"line_number":"3.3.0","speaker":"Second Murderer","text_entry":"Enter BANQUO, and FLEANCE with a torch"},{"line_id":57047,"play_name":"Macbeth","speech_number":11,"line_number":"3.3.21","speaker":"Third Murderer","text_entry":"'Tis he."},{"line_id":57048,"play_name":"Macbeth","speech_number":12,"line_number":"3.3.22","speaker":"First Murderer","text_entry":"Stand to't."},{"line_id":57049,"play_name":"Macbeth","speech_number":13,"line_number":"3.3.23","speaker":"BANQUO","text_entry":"It will be rain to-night."},{"line_id":57050,"play_name":"Macbeth","speech_number":14,"line_number":"3.3.24","speaker":"First Murderer","text_entry":"Let it come down."},{"line_id":57051,"play_name":"Macbeth","speech_number":14,"line_number":"3.3.0","speaker":"First Murderer","text_entry":"They set upon BANQUO"},{"line_id":57052,"play_name":"Macbeth","speech_number":15,"line_number":"3.3.25","speaker":"BANQUO","text_entry":"O, treachery! Fly, good Fleance, fly, fly, fly!"},{"line_id":57053,"play_name":"Macbeth","speech_number":15,"line_number":"3.3.26","speaker":"BANQUO","text_entry":"Thou mayst revenge. O slave!"},{"line_id":57054,"play_name":"Macbeth","speech_number":15,"line_number":"3.3.0","speaker":"BANQUO","text_entry":"Dies. FLEANCE escapes"},{"line_id":57055,"play_name":"Macbeth","speech_number":16,"line_number":"3.3.27","speaker":"Third Murderer","text_entry":"Who did strike out the light?"},{"line_id":57056,"play_name":"Macbeth","speech_number":17,"line_number":"3.3.28","speaker":"First Murderer","text_entry":"Wast not the way?"},{"line_id":57057,"play_name":"Macbeth","speech_number":18,"line_number":"3.3.29","speaker":"Third Murderer","text_entry":"There's but one down; the son is fled."},{"line_id":57058,"play_name":"Macbeth","speech_number":19,"line_number":"3.3.30","speaker":"Second Murderer","text_entry":"We have lost"},{"line_id":57059,"play_name":"Macbeth","speech_number":19,"line_number":"3.3.31","speaker":"Second Murderer","text_entry":"Best half of our affair."},{"line_id":57060,"play_name":"Macbeth","speech_number":20,"line_number":"3.3.32","speaker":"First Murderer","text_entry":"Well, let's away, and say how much is done."},{"line_id":57061,"play_name":"Macbeth","speech_number":20,"line_number":"3.3.0","speaker":"First Murderer","text_entry":"Exeunt"}]
