[{"line_id":55780,"play_name":"Macbeth","speech_number":405,"line_number":"1.1.0","speaker":"ADRIANO DE ARMADO","text_entry":"ACT I"},{"line_id":55781,"play_name":"Macbeth","speech_number":405,"line_number":"1.1.0","speaker":"ADRIANO DE ARMADO","text_entry":"SCENE I. A desert place."},{"line_id":55782,"play_name":"Macbeth","speech_number":405,"line_number":"1.1.0","speaker":"ADRIANO DE ARMADO","text_entry":"Thunder and lightning. Enter three Witches"},{"line_id":55783,"play_name":"Macbeth","speech_number":1,"line_number":"1.1.1","speaker":"First Witch","text_entry":"When shall we three meet again"},{"line_id":55784,"play_name":"Macbeth","speech_number":1,"line_number":"1.1.2","speaker":"First Witch","text_entry":"In thunder, lightning, or in rain?"},{"line_id":55785,"play_name":"Macbeth","speech_number":2,"line_number":"1.1.3","speaker":"Second Witch","text_entry":"When the hurlyburly's done,"},{"line_id":55786,"play_name":"Macbeth","speech_number":2,"line_number":"1.1.4","speaker":"Second Witch","text_entry":"When the battle's lost and won."},{"line_id":55787,"play_name":"Macbeth","speech_number":3,"line_number":"1.1.5","speaker":"Third Witch","text_entry":"That will be ere the set of sun."},{"line_id":55788,"play_name":"Macbeth","speech_number":4,"line_number":"1.1.6","speaker":"First Witch","text_entry":"Where the place?"},{"line_id":55789,"play_name":"Macbeth","speech_number":5,"line_number":"1.1.7","speaker":"Second Witch","text_entry":"Upon the heath."},{"line_id":55790,"play_name":"Macbeth","speech_number":6,"line_number":"1.1.8","speaker":"Third Witch","text_entry":"There to meet with Macbeth."},{"line_id":55791,"play_name":"Macbeth","speech_number":7,"line_number":"1.1.9","speaker":"First Witch","text_entry":"I come, Graymalkin!"},{"line_id":55792,"play_name":"Macbeth","speech_number":8,"line_number":"1.1.10","speaker":"Second Witch","text_entry":"Paddock calls."},{"line_id":55793,"play_name":"Macbeth","speech_number":9,"line_number":"1.1.11","speaker":"Third Witch","text_entry":"Anon."},{"line_id":55794,"play_name":"Macbeth","speech_number":10,"line_number":"1.1.12","speaker":"ALL","text_entry":"Fair is foul, and foul is fair:"},{"line_id":55795,"play_name":"Macbeth","speech_number":10,"line_number":"1.1.13","speaker":"ALL","text_entry":"Hover through the fog and filthy air."},{"line_id":55796,"play_name":"Macbeth","speech_number":10,"line_number":"1.1.0","speaker":"ALL","text_entry":"Exeunt"}]
